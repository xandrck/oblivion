  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    <div id="wrapper">
      <div id="page-wrapper">
        <div class=row>
          <div class="col-md-12">
            <%= render partial: 'common/menu' if current_user %>
            <%= render partial: 'common/flash' %>
            <%= yield %>
          </div>
        </div>
      </div>
    </div>
</body>
</html>
