.col-md-4.col-md-offset-4
  .text-center
    %h3 Bookmarks list
.col-md-12
  %table.table.table-striped.table-hover
    %thead
      %tr
        %th.col-md-4 Name
        %th.col-md-4 Group name
        %th.col-md-4 Action
    %tbody
      = render partial: 'table', collection: @bookmarks, as: :bookmark
